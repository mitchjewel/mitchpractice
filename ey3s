function validateLoginA() {
  const user = document.getElementById("usernameA").value;
  const pass = document.getElementById("passwordA").value;

  if (user === "admin" && pass === "1234") {
    alert("Login successful! (Set A)");
    window.location.href = "portfolioA.html";
    return false;
  } else {
    alert("Invalid credentials. Try again.");
    return false;
  }
}
